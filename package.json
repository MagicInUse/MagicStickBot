{
  "name": "magicstickbot-twitch-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev --prefix client\" \"npm run dev --prefix server\"",
    "build": "cd client && vite build && cd ../server && tsc",
    "start": "cd server && ts-node src/server.ts"
  },
  "dependencies": {
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sequelize": "^6.37.5",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  }
}
